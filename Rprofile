if(interactive()){
       #library(colorout)
       library(setwidth)
       options(vimcom.verbose = 1) # optional
       library(vimcom)
}

#cat(".Rprofile: setting default repository\n")
r = getOption("repos")
#r["CRAN"]="http//cran.revolutionanalytics.com"
r["CRAN"]="http://cran.uk.r-project.org"
options(repos = r)
rm(r)
